{
  "name": "avs-check",
  "version": "1.0.0",
  "type": "module",
  "description": "This is an age verification service check, decentralized, anonymous",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon --watch src --ext ts --exec tsx src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "npx eslint \"src/**/*.ts\"",
    "lint:fix": "npx eslint \"src/**/*.ts\" --fix",
    "format": "npx prettier --write \"src/**/*.{ts,json}\"",
    "format:check": "npx prettier --check \"src/**/*.{ts,json}\"",
    "prepare": "husky install",
    "clean": "rm -rf dist coverage",
    "services:up": "docker-compose -f docker-compose.dev.yml up -d",
    "services:down": "docker-compose -f docker-compose.dev.yml down",
    "services:logs": "docker-compose -f docker-compose.dev.yml logs -f"
  },
  "keywords": [
    "age-verification",
    "decentralized",
    "anonymous",
    "privacy",
    "identity"
  ],
  "author": "muka-dev",
  "license": "ISC",
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^6.1.0",
    "@prisma/client": "^5.22.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/helmet": "^0.0.48",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^25.0.3",
    "@types/snarkjs": "^0.7.9",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "hardhat": "^2.28.3",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "lint-staged": "^16.2.7",
    "nodemon": "^3.1.11",
    "prettier": "^3.7.4",
    "prisma": "^5.22.0",
    "supertest": "^7.2.2",
    "swc-loader": "^0.2.6",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@noble/hashes": "^2.0.1",
    "@noble/post-quantum": "^0.5.4",
    "circomlib": "^2.0.5",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "ethers": "^6.16.0",
    "express": "^5.2.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.9.1",
    "jsonwebtoken": "^9.0.3",
    "morgan": "^1.10.1",
    "snarkjs": "^0.7.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  }
}
